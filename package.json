    "@babel/preset-typescript": "^7.12.13",
    "@babel/runtime": "^7.12.18",
    "@rollup/plugin-babel": "^5.2.3",
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "rollup": "^2.38.5",
    "typescript": "^4.1.5"
  }
